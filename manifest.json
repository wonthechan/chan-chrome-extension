{
  "name": "ChanNewTab",
  
  "version": "1.0",
  
  "description": "This Chrome extension greets the user each time they open a new tab",

  "author": "Yechan Lim",
  
  "manifest_version": 2,

  "browser_action": {
  	"default_icon": "tab-icon.png",
  	"default_title": "Have a great day"
  },
  
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  
  "permissions": [
     "activeTab"
   ]
}